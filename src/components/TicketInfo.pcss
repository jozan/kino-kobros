.root {

    h1 {
        text-align: center;
    }

    table {
        width: 100%;
        border: 1px solid rgb(0,0,0);
        margin: 1rem 0;

        th, td {
            padding: .25rem;
        }

        th {
            text-align: right;
        }

        td {
            text-align: left;
        }
    }

}
